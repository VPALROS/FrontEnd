
@{
    ViewBag.Title = "Index";
}
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/knockout")
<h2>Exercise 3 (TypeScript + Knockout)</h2>

<h1>
    <div class="container customContainer col-xl-12">
        <div class="logo col-xl-6"></div>
        <div class="col-xl-6" style="float:right">
            <div class="col-xl-6">
                <div class="result" data-bind="text:price"> </div>
                <label class="result">€</label>
            </div>
            <br />
            <div class="col-xl-6">
                <div class="result" data-bind="text:differencePercentage"></div>
                <label data-bind="css:icon"></label>
            </div>
        </div>
    </div>
</h1>

@Scripts.Render("~/bundles/ViewModelsTypeScript")

